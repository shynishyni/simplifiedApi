{
  "builds": [
    {
      "src": "simplifiedApi/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "simplifiedApi/wsgi.py"
    }
  ],
  "env": {
    "SECRET_KEY": "@your-secret-key",
    "DEBUG": "false",
    "ALLOWED_HOSTS": "['your-vercel-url.vercel.app']",
    "DATABASE_URL": "@your-database-url"
  }
}
