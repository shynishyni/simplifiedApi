{
    "builds":[{
        "src":"simplifiedApi/wsgi.py",
        "use":"@vercel/python",
        "config":{ "maxLambdaSize": "15mb",
                    "runtime": "python3.9",
                    "install-command": "pip install --target /vercel/path0 --upgrade -r requirements.txt",
                    "build-commands": [
                        "sudo apt-get update",
                        "sudo apt-get install -y libmariadb-dev libssl-dev pkg-config"
                ] }
    }],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "simplifiedApi/wsgi.py"
        }
    ]
}
